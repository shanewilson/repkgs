(library (name RepkgsCliLibrary) (public_name repkgs--cli.library)
    (modules (:standard \ RepkgsCliLibraryPesyModules))
    (libraries repkgs--cli.library.pesy-modules)
    (flags -open Base -open RepkgsCliLibraryPesyModules)
    (preprocess (pps lwt_ppx)))
(library (public_name repkgs--cli.library.pesy-modules)
    (name RepkgsCliLibraryPesyModules) (modules RepkgsCliLibraryPesyModules)
    (libraries base console.lib lwt pastel.lib) (preprocess (pps lwt_ppx)))
(rule
    (with-stdout-to RepkgsCliLibraryPesyModules.re
        (run echo
             "module Base = Base;\
            \nmodule Console = Console;\
            \nmodule Pastel = Pastel;\
            \nmodule Lwt = Lwt;")))
(include_subdirs unqualified)
