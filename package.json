{
  "name": "@repkgs/repkgs",
  "version": "0.0.0",
  "repository": "https://github.com/peykio/repkgs",
  "keywords": [
    "bucklescript",
    "reason",
    "reasonml"
  ],
  "license": "MIT",
  "jest": {
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/__tests__/"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!@glennsl/*|bs-platform|wonka|decco).+\\.js$"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/plugin-transform-modules-commonjs": "^7.8.3",
    "@glennsl/bs-jest": "^0.4.9",
    "bs-platform": "^7.0.2-dev.1",
    "gentype": "^3.10.0",
    "typescript": "^3.7.5"
  },
  "scripts": {
    "clean": "bsb -clean-world",
    "build": "bsb -make-world",
    "watch": "bsb -make-world -w",
    "test": "jest",
    "problems": "tsc --noEmit"
  },
  "dependencies": {
    "decco": "^1.1.1",
    "execa": "^4.0.0",
    "fast-glob": "^3.1.1",
    "js-yaml": "^3.13.1",
    "wonka": "^4.0.7"
  }
}
