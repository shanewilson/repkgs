{
  "name": "@repkgs/repkgs",
  "version": "0.0.0",
  "sources": [
    {
      "dir": "src",
      "subdirs": [
        "bs",
        "bs/Ink",
        "library",
        "cli",
        "components",
        "components/Error"
      ]
    },
    {
      "dir": "__tests__",
      "type": "dev"
    }
  ],
  "package-specs": [
    {
      "module": "es6",
      "in-source": true
    }
  ],
  "gentypeconfig": {
    "language": "typescript",
    "module": "es6",
    "importPath": "relative"
  },
  "suffix": ".bs.js",
  "bs-dependencies": [
    "decco",
    "@elliottcable/bs-cmdliner",
    "reason-react"
  ],
  "ppx-flags": [
    "decco/ppx"
  ],
  "bsc-flags": [
    "-bs-no-version-header",
    "-bs-g",
    "-bs-super-errors",
    "-warn-error",
    "@A",
    "-w",
    "-4-9-30-40-41-42-44-48-102",
    "-open",
    "Belt"
  ],
  "refmt": 3,
  "reason": {
    "react-jsx": 3
  }
}