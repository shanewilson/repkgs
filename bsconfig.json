{
  "name": "@repkgs/repkgs",
  "version": "0.0.0",
  "sources": [
    {
      "dir": "src",
      "subdirs": [
        "bs",
        "library",
        "cli",
        "Repkgs",
        "RepkgsCli"
      ],
      "public": [
        "Repkgs"
      ]
    },
    {
      "dir": "__tests__",
      "type": "dev"
    }
  ],
  "package-specs": [
    {
      "module": "es6",
      "in-source": true
    }
  ],
  "gentypeconfig": {
    "language": "typescript",
    "module": "es6",
    "importPath": "relative"
  },
  "suffix": ".bs.js",
  "bs-dependencies": [
    "wonka",
    "decco",
    "@elliottcable/bs-cmdliner"
  ],
  "bs-dev-dependencies": [
    "@glennsl/bs-jest"
  ],
  "ppx-flags": [
    "decco/ppx"
  ],
  "bsc-flags": [
    "-bs-no-version-header",
    "-bs-g",
    "-bs-super-errors",
    "-warn-error",
    "@A",
    "-w",
    "-4-9-30-40-41-42-44-102-48",
    "-open",
    "Belt"
  ],
  "refmt": 3
}